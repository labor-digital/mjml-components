        />

        <mj-section>
            <mj-column>
                <mj-divider mj-class="divider" />
            </mj-column>
        </mj-section>

        <labor-adobe-section padding-top="40px" padding-bottom="40px" section-bg-class="heading-bg">
            <mj-column>
                <labor-adobe-typo-heading-three>
                    EdEx Article - with padding-top/bottom
                </labor-adobe-typo-heading-three>
            </mj-column>
        </labor-adobe-section>

        <mj-section>
            <mj-column>
                <mj-divider mj-class="divider" />
            </mj-column>
        </mj-section>

        <labor-adobe-edex-article
                section-bg-class="content-bg"
                headline="Design a 2025 moodboard"
                category="Template"
                text="Students will build visual storytelling skills as they use photos and design elements to create moodboards that set the tone for the year ahead."
                cta="Get started"
                href="#"
                image-src="../src/Adobe/Assets/Components/Custom/EdexArticle/article_image.gif"
                image-src-mobile="../src/Adobe/Assets/Components/Custom/EdexArticle/article_image_m.gif"
                product="express"
                padding-bottom="150px"
                padding-top="150px"
        />

        <mj-section>
            <mj-column>
                <mj-divider mj-class="divider" />
            </mj-column>
        </mj-section>

        <labor-adobe-section padding-top="40px" padding-bottom="40px" section-bg-class="heading-bg">
            <mj-column>
                <labor-adobe-typo-heading-three>
                    EdEx Article
                </labor-adobe-typo-heading-three>
            </mj-column>
        </labor-adobe-section>

        <mj-section>
            <mj-column>
                <mj-divider mj-class="divider" />
            </mj-column>
        </mj-section>

        <labor-adobe-edex-article
                section-bg-class="content-bg"
                headline="Design a 2025 moodboard"
