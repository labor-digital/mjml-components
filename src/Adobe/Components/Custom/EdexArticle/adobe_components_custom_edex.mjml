                image-src-mobile="../src/Adobe/Assets/Components/Custom/EdexArticle/article_image_m.gif"
                product="premiere_pro"
        />

        <mj-section>
            <mj-column>
                <mj-divider mj-class="divider" />
            </mj-column>
        </mj-section>


        <labor-adobe-section padding-bottom="100px" section-bg-class="heading-bg">

        </labor-adobe-section>

    </mj-body>
</mjml>